{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": [".", "./README.md"],
        "exclude": ["./node_modules"],
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }
}